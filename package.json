{
  "name": "sasuke-dinner-notify",
  "version": "3.0.0",
  "description": "Fetch @ekusas55000's \"sasuke-dinner\" and notify a specific channel on the Discord server.",
  "license": "MIT",
  "author": "Tomachi <tomachi@tomacheese.com>",
  "main": "index.js",
  "repository": "git@github.com:tomacheese/sasuke-dinner-notify.git",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev --poll -r tsconfig-paths/register ./src/main.ts",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/",
    "compile": "tsc -p .",
    "clean": "rimraf dist output",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@book000/node-utils": "1.10.2",
    "@book000/twitterts": "0.45.6",
    "@types/node": "20.9.1",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "49.0.0",
    "prettier": "3.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "twitter-d": "0.6.0",
    "typescript": "5.2.2",
    "yarn-run-all": "3.1.1"
  }
}
